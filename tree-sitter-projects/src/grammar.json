{
  "name": "projects",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "project"
      }
    },
    "project": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "SYMBOL",
          "name": "text"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "project_children"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "child_project": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "##"
        },
        {
          "type": "SYMBOL",
          "name": "text"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "project_grandchildren"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "grandchild_project": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "###"
        },
        {
          "type": "SYMBOL",
          "name": "text"
        }
      ]
    },
    "project_children": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "child_project"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        }
      ]
    },
    "project_grandchildren": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "grandchild_project"
        },
        {
          "type": "SYMBOL",
          "name": "child_comment"
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "+"
        },
        {
          "type": "SYMBOL",
          "name": "text"
        }
      ]
    },
    "child_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "++"
        },
        {
          "type": "SYMBOL",
          "name": "text"
        }
      ]
    },
    "text": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_ ]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

